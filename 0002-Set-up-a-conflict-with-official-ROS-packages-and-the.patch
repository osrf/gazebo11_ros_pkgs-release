From 13cdb53ad8802b84d57b4f48b5849ac605b4cf27 Mon Sep 17 00:00:00 2001
From: Jose Luis Rivero <jrivero@osrfoundation.org>
Date: Fri, 1 May 2020 01:34:01 +0200
Subject: [PATCH 2/3] Set up a conflict with official ROS packages and the two
 previous gazebo versions

---
 debian/control.em | 1 +
 1 file changed, 1 insertion(+)

diff --git a/debian/control.em b/debian/control.em
index 18d4fc13..9f62273e 100644
--- a/debian/control.em
+++ b/debian/control.em
@@ -9,6 +9,7 @@ Standards-Version: 3.9.2
 Package:@(Package.replace('gazebo-','gazebo11-'))
 Architecture: any
 Depends: ${shlibs:Depends}, ${misc:Depends}, @(', '.join(Depends))
+Conflicts: @(Package) , @(Package.replace('gazebo-','gazebo10-')), @(Package.replace('gazebo-','gazebo9-'))/
 @[if Conflicts]Conflicts: @(', '.join(Conflicts))@\n@[end if]@
 @[if Replaces]Replaces: @(', '.join(Replaces))@\n@[end if]@
 Description: @(Description)
